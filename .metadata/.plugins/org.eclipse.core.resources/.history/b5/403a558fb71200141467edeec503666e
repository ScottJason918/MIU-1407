var mainWindow = Ti.UI.createWindow({
	backgroundColor: "#ffffff",
	title: "Movie App"
});

var logo = Ti.UI.createImageView ({
	image: "Picture/Logo.jpg",
	top: 0
});
var navWindow = Ti.UI.iOS.createNavigationWindow({
	window: mainWindow
});

navWindow.open();
mainWindow.add(logo);
